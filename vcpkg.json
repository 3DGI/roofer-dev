{
  "name": "roofer",
  "version-string": "1.0.0",
  "features": {
    "spdlog": {
      "description": "Use spdlog as logging backend.",
      "dependencies": [
        "spdlog"
      ]
    },
    "test": {
      "description": "Build tests.",
      "dependencies": [
        "catch2"
      ]
    }
  },
  "dependencies": [
    {
      "name": "nlohmann-json",
      "version>=": "3.11.3"
    },
    "boost-geometry",
    "boost-format",
    "lastools",
    "cgal",
    "eigen3",
    "proj",
    "geos",
    "fmt",
    {
      "name": "gdal",
      "features": [
        "recommended-features",
        "postgresql"
      ]
    },
    {
      "name": "libfork",
      "version>=": "3.8.0"
    }
  ],
  "overrides": [
    {
      "name": "nlohmann-json",
      "version": "3.11.3"
    },
    {
      "name": "boost-geometry",
      "version": "1.84.0"
    },
    {
      "name": "boost-format",
      "version": "1.84.0"
    }
  ],
  "builtin-baseline": "9224b3bbd8df24999d85720b1d005dd6f969ade0"
